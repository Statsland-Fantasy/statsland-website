# Backend API Configuration
REACT_APP_API_BASE_URL=http://localhost:8080

# Set to 'true' to use mock data instead of API (for development without backend)
REACT_APP_USE_MOCK_DATA=false

# Auth0 Configuration
# Get these from your Auth0 Dashboard: https://manage.auth0.com/
REACT_APP_AUTH0_DOMAIN=auth0-domain
REACT_APP_AUTH0_CLIENT_ID=auth0-clientid
REACT_APP_AUTH0_AUDIENCE=auth0-audience

# Google Analytics Configuration
REACT_APP_GA_MEASUREMENT_ID=G-XXXXXXXXXX
# Set to 'true' to enable GA tracking and debug logging in development mode
REACT_APP_GA_DEBUG=false
